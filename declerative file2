pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo " i am in builds"
            }
        }
        stage('Test') { 
            steps {
                 echo " i am in tests"
            }
        }
        stage('Deploy') { 
            steps {
                echo " i am in deploys" 
            }
        }
    }
}
